  "author": "maxgrossman <m.grossman1813@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-page": "node .build_scripts/buildHTML.js",
    "serve": "DS_ENV=development node .build_scripts/buildHTML.js && gulp serve",
    "build": "DS_ENV=production NODE_ENV=production gulp",
    "stage": "DS_ENV=staging NODE_ENV=staging gulp"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
