    "browserify": "^14.1.0",
    "del": "^2.2.2",
    "envify": "^4.0.0",
    "eslint": "^4.1.1",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.6",
